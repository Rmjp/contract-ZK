pragma solidity ^0.8.20;

contract VerifyProof {

    function verifyProof(
        uint256[2] memory a,
        uint256[2][2] memory b,
        uint256[2] memory c,
        uint256[] memory input
    ) external pure returns (bool r) {
        // Check a
        if (
            a[0] != 1979496885708545220048407894739534586820693546962577086375146656121620436553 ||
            a[1] != 10182651848445521920455386271952485105701096192576758030723861398062831737620
        ) {
            return false;
        }

        // Check b
        if (
            b[0][0] != 6955347466464006692082152698856965575428555261888236858585297536350430543492 ||
            b[0][1] != 5219327041220178716156284507012885562917727048768613584202612911587413814844 ||
            b[1][0] != 6974615560404434844936286813229041646186689183989822996438562719930338961930 ||
            b[1][1] != 7595541542557352072229760573663796577306327312902272814596751641463225509355
        ) {
            return false;
        }

        // Check c
        if (
            c[0] != 10448451144818640130569970273933711549615683306795936399661126090059314446786 ||
            c[1] != 15198414401460201569022592153536752939049919601410570779412138256769054732609
        ) {
            return false;
        }

        // Check input array length first
        if (input.length != 4) {
            return false;
        }

        // Check each input element
        if (
            input[0] != 23059336182092717530402538631517012974515776249001969233049292365119689217 ||
            input[1] != 10093801711445124399296655279034210478654769270697792394528486326197825707083 ||
            input[2] != 288648600274475711174233815269229986964406022137315219531346496269730274570 ||
            input[3] != 1
        ) {
            return false;
        }

        return true;
    }
}